Add Batch
<table>
        <tr>
            <th>Batch_ID</th>
            <th>Batch_Name</th>
            <th>Action</th>
        </tr>
<% data.forEach((batch)=>{ %>
    
        <tr>
            <td>    <%= batch.id; %></td>
            <td>    <%= batch.batch; %></td>
            <td><a href="/admin/delete/<%= batch.id %>"><button>Delete</button></a></td>
        </tr>
 

<% }) %>
</table>

<body>
    <div style="margin: 20px;">
        <form action="/admin/addbatch" method="POST" >
            <input type="text" name="batch" placeholder="BatchName" required>
            <button>Add</button>
        </form>
        <br>
    </div>
    
</body>



