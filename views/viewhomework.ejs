<% include partials/bshead %>
<hr>

View Homework
<table style="border:solid 1px;">
    <tr>
        <th>Homework_ID</th>
        <th>Title</th>
        <th>Batch_Name</th>
        <th>Assigned Date</th>
        <th>Deadline</th>
        <th>Teacher</th>
        <th>Action</th>
    </tr>
<% data.forEach((homework)=>{ %>
    <tr>
        <td>    <%= homework.id; %></td>
        <td>     <a href="/viewhomework/<%= homework.id; %>"><b> <%= homework.Title; %> </b></a>   </td>
        <td>    <%= homework.batch; %></td>
        <td>    <%= homework.Assigned_Date.slice(0,11); %></td>
        <td>    <%= homework.Deadline; %></td>
        <td>    <%= homework.Teacher; %></td>
        <td><a href="/admin/deletehomework/<%= homework.id %>"><button>Delete</button></a></td>
    </tr>
<% }) %>
</table>
<% include partials/bsfoot %>
